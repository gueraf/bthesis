	keywords = {Chemical reaction theory, Number theory, Numerical modeling, Poisson's equation, Probability theory},
	pages = {054104},
	file = {Full Text PDF:/home/fabian/.zotero/zotero/del86n4i.default/zotero/storage/3KM2DIK6/Cao et al. - 2005 - Avoiding negative populations in explicit Poisson .pdf:application/pdf;Snapshot:/home/fabian/.zotero/zotero/del86n4i.default/zotero/storage/85QWK75A/1.html:text/html}
