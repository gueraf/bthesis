	file = {ScienceDirect Full Text PDF:/home/fabian/.zotero/zotero/del86n4i.default/zotero/storage/WHSFMTMR/Stundzia and Lumsden - 1996 - Stochastic Simulation of Coupled Reaction–Diffusio.pdf:application/pdf;ScienceDirect Snapshot:/home/fabian/.zotero/zotero/del86n4i.default/zotero/storage/SJ79GVW3/S0021999196901680.html:text/html}
}

