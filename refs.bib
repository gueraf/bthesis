	file = {ACM Full Text PDF:/home/fabian/.zotero/zotero/del86n4i.default/zotero/storage/ZMEGVC9B/Lee et al. - 2010 - Debunking the 100X GPU vs. CPU Myth An Evaluation.pdf:application/pdf}
